{
    "builds":[
        {
            "src": "api/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ],
    "crons": [
        {
            "path": "/refresh-proxy",
            "schedule": "0 12 * * *"
        }
    ]
}